#!/bin/bash
awk -v w="$WATER" -v s="$STIR" 'BEGIN{wa="ewwatratewa";st="ti.itirtrtr";b="bestchol";n=0;for(i=1;i<=length(w);i++){for(j=1;j<=length(wa);j++)if(substr(wa,j,1)==substr(w,i,1)){n=n*length(wa)+j-1;break}};for(i=1;i<=length(s);i++){for(j=1;j<=length(st);j++)if(substr(st,j,1)==substr(s,i,1)){n=n*length(st)+j-1;break}};r="";while(n){r=substr(b,n%length(b)+1,1) r;n=int(n/length(b))};if(r=="")r=substr(b,1,1);print r}'
